{
    "name": "zfr/zfr-sqs-worker",
    "type": "library",
    "license": "MIT",
    "description" : "Thin abstraction around AWS SQS",
    "keywords": [
        "sqs",
        "middleware",
        "expressive",
        "elastic beanstalk"
    ],
    "homepage": "https://github.com/zf-fr/zfr-sqs-worker",
    "authors": [
        {
            "name": "MichaÃ«l Gallego",
            "email": "mic.gallego@gmail.com",
            "homepage": "http://www.michaelgallego.fr"
        }
    ],
    "require": {
        "php": "~7.0",
        "aws/aws-sdk-php": "~3.0",
        "container-interop/container-interop": "^1.1",
        "psr/http-message": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8"
    },
    "autoload": {
        "psr-4": {
            "ZfrSqsWorker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
